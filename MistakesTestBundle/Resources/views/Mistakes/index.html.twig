<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Mistakes Bundle{% endblock %}</title>
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        <script type="text/javascript" src="https://www.google.com/jsapi"></script>
        <script type="text/javascript" src="js/codigo.js"></script>
        <script type="text/javascript">
            google.load("visualization", "1", {packages:["corechart", 'table', 'treemap', 'gauge']});
            google.load('jquery', '1.6.0');
        </script>
    </head>



{% block body %} 

<script>

function ChartCallBackHandlerchart2() {

	projectArray = new Array();
	{% for key, charts2 in charts['0']['rows']  %}
		projectArray['{{key}}'] = "{{charts2['c']['0']['v']|e }}";
 	{% endfor %}
 	
	selectrow = chartchart2.getSelection()[0].row;		
	{% for proyect in charts['1'] %}
		if ((projectArray[selectrow]) == '{{ proyect.name|e }}') {
			 window.open('http://kunstmaan.airbrake.io/projects/' + '{{ proyect.abId|e }}' + '/deploys.xml?auth_token=808b45ec1d7854563c3bb2fab160e5b90f5e1ae3 ' , 'new');
		}
	{% endfor %}
		
}


$(function() {
	{{ gchart_column_chart(charts[0], 'chart2', 800, 500, 'vertical') }}
});

</script>
	
     
<div id="chart2">&nbsp;</div>

    
{% endblock %}